/* About Us Section Enhancements */
.about-us-text {
    position: relative;
    background: var(--card-bg);
    border-radius: 15px;
    box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
    overflow: hidden;
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    padding: 20px;
    margin: 20px 0;
    height: 300px; /* Set a height for the boxes */
    color: var(--text-color);
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    text-align: center;
    animation: colorChange 10s infinite alternate; /* Animation for color change */
}

/* Color Change Animations */
@keyframes colorChange {
    0% {
        background-color: rgba(56, 102, 65, 0.8); /* Dark green for Vision */
    }
    25% {
        background-color: rgba(106, 153, 78, 0.8); /* Lighter green for Vision */
    }
    50% {
        background-color: rgba(167, 201, 87, 0.8); /* Yellow-green for Mission */
    }
    75% {
        background-color: rgba(255, 255, 255, 0.8); /* White for Impact */
    }
    100% {
        background-color: rgba(255, 255, 255, 0.8); /* White for Goals */
    }
}

.about-us-text.vision {
    animation: colorChangeVision 10s infinite alternate; /* Specific animation for Vision */
}

@keyframes colorChangeVision {
    0% {
        background-color: rgba(56, 102, 65, 0.8); /* Dark green */
    }
    100% {
        background-color: rgba(106, 153, 78, 0.8); /* Lighter green */
    }
}

.about-us-text.mission {
    animation: colorChangeMission 10s infinite alternate; /* Specific animation for Mission */
}

@keyframes colorChangeMission {
    0% {
        background-color: rgba(167, 201, 87, 0.8); /* Yellow-green */
    }
    100% {
        background-color: rgba(255, 223, 186, 0.8); /* Light peach */
    }
}

.about-us-text.impact {
    animation: colorChangeImpact 10s infinite alternate; /* Specific animation for Impact */
}

@keyframes colorChangeImpact {
    0% {
        background-color: rgba(255, 255, 255, 0.8); /* White */
    }
    100% {
        background-color: rgba(173, 216, 230, 0.8); /* Light blue */
    }
}

