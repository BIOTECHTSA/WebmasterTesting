        grid-template-columns: 1fr;
    }
    
    .meal-plans-grid {
        grid-template-columns: repeat(2, 1fr);
        place-items: center;
    }
    
    /* Adjust the 4th and 5th meal plans for tablet layout */
    .meal-plan:nth-child(4) {
        grid-column: 1 / span 1;
        grid-row: 2;
    }
    
    .meal-plan:nth-child(5) {
        grid-column: 2 / span 1;
        grid-row: 2;
    }
    
    .meal-plan {
        max-width: 320px;
    }
    
    .meal-plan.featured {
        transform: scale(1);
        border-width: 2px;
    }
    
    .meal-plan.featured:hover {
        transform: translateY(-5px);
    }
    
    .meal-plan.featured::before {
        top: -10px;
        right: 15px;
        padding: 3px 10px;
        font-size: 0.7rem;
    }
}

