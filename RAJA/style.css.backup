        padding: 0;
        margin: 20px 0;
    }
    
    .road-path {
        display: none;
    }
    
    .road-point {
        position: static;
        width: 100%;
        margin: 15px 0;
        padding: 20px;
        text-align: left !important;
        transform: none !important;
        opacity: 1;
        left: 0 !important;
        right: 0 !important;
        animation: slideUp 0.3s ease forwards;
        border-left: 4px solid #e67e22;
    }

    .road-point::before,
    .road-point::after {
        display: none;
    }

    .road-point h3::before {
        content: counter(farm-item);
        counter-increment: farm-item;
        display: inline-block;
        width: 30px;
        height: 30px;
        background: linear-gradient(45deg, #e67e22, #f39c12);
        color: rgb(255, 255, 255);
        border-radius: 50%;
        text-align: center;
        line-height: 30px;
        margin-right: 10px;
        font-size: 0.9em;
    }

    .road-path {
        counter-reset: farm-item;
    }

    @keyframes slideUp {
        from {
            opacity: 0;
            transform: translateY(20px);
        }
        to {
            opacity: 1;
            transform: translateY(0);
        }
    }

    .road-point:hover {
        transform: none !important;
        box-shadow: 0 5px 15px rgba(0,0,0,0.15);
